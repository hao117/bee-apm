<properties>
    <property name="error-pie" tip="错误占比">
        <![CDATA[
        {
          "size": 0,
          "aggs": {
            "errorCount": {
              "terms": {
                "field": "attributes.app_name.keyword",
                "order": {
                  "_count": "desc"
                },
                "size": 9
              }
            }
          }
        }
        ]]>
    </property>
    <property name="error-line" tip="异常趋势图">
        <![CDATA[
        {
          "size": 0,
          "aggs": {
            "app_group": {
              "terms": {
                "field": "attributes.app_name.keyword",
                "size": 10
              },
              "aggs": {
                "time_group": {
                  "date_histogram": {
                    "field": "startTime",
                    "interval": "1h",
                    "extended_bounds": {
                        "min": #[minTime],
                        "max": #[maxTime]
                    }
                  }
                }
              }
            }
          }
        }
        ]]>
    </property>
    <property name="request-bar" tip="请求耗时区间统计 ">
        <![CDATA[
        {
            "size":0,
            "aggs":{
                "req_bar":{
                    "range":{
                        "field":"duration",
                        "format":"0",
                        "ranges":[
                            {
                                "from": 0,
                                "to": 10
                            },
                            {
                                "from": 10,
                                "to": 20
                            },
                            {
                                "from": 20,
                                "to": 50
                            },
                            {
                                "from": 50,
                                "to": 100
                            },
                            {
                                "from": 100,
                                "to": 200
                            },
                            {
                                "from": 200,
                                "to": 500
                            },
                            {
                                "from": 500,
                                "to": 1000
                            },
                            {
                                "from": 1000,
                                "to": 2000
                            },
                            {
                                "from": 2000
                            }
                        ]
                    }
                }
            }
        }
        ]]>
    </property>
    <property name="request-line" tip="请求量趋势图">
        <![CDATA[
            {
              "size": 0,
              "aggs": {
                "app_group": {
                  "terms": {
                    "field": "attributes.app_name.keyword",
                    "size": 10
                  },
                  "aggs": {
                    "time_group": {
                      "date_histogram": {
                        "field": "startTime",
                        "interval": "1h",
                        "extended_bounds": {
                            "min": #[minTime],
                            "max": #[maxTime]
                        }
                      }
                    }
                  }
                }
              }
            }
        ]]>
    </property>
    <property >

    </property>
</properties>